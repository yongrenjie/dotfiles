parse_git_branch() {
     git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/ (\1)/'
}
export PS1="\[\e[1;32m\]\u\[\e[m\]@\[\e[1;33m\]\h\[\e[m\]:\\[\e[1;35m\]\w\[\e[m\]\[\e[1;31m\]\$(parse_git_branch)\[\e[m\] \[\e[1;35m\]$ \[\e[m\]"
